<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
		xmlns:em="http://www.mozilla.org/2004/em-rdf#">

	<Description about="urn:mozilla:install-manifest">
		<em:name>Selenium-IDE-Addon</em:name>
		<em:version>1.0</em:version>
		<em:description>Extends Selenium IDE</em:description>
		<em:creator>Rudolf</em:creator>
		<em:id>selenium-ide-extension@mouseless.de</em:id>
		
		<em:targetApplication>
			<!-- Firefox -->
			<Description>
				<em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
				<em:minVersion>3.5</em:minVersion>
				<em:maxVersion>4.0b8</em:maxVersion>
			</Description>
		</em:targetApplication>
		
		<em:iconURL></em:iconURL>
		<em:homepageURL></em:homepageURL>
      
    <!-- this is an Se-IDE plugin, so we need to specify it as a requirement -->
    <em:requires>   
      <Description>
        <em:id>{a6fd85ed-e919-4a43-a5af-8da18bda539f}</em:id>
        <em:minVersion>1.0.8</em:minVersion>
        <em:maxVersion>1.*</em:maxVersion>
      </Description>  
    </em:requires>
      
	</Description>
</RDF>
